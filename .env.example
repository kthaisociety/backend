GOOGLE_CLIENT_ID=<GOOGLE_CLIENT_ID>
GOOGLE_CLIENT_SECRET=<GOOGLE_CLIENT_SECRET>

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=kthais
DB_SSLMODE=disable

# Server configuration
SERVER_PORT=8080 

# Add a strong session key (generate with openssl rand -base64 32)
SESSION_KEY=<SESSION_KEY>

# Set to true in production
SECURE_COOKIE=false

# OAuth Configuration
GOOGLE_ALLOWED_DOMAINS=localhost:3000  # Restrict to specific domains
OAUTH_STATE_TIMEOUT=600                         # State timeout in seconds
OAUTH_RATE_LIMIT_REQUESTS=5                     # Requests per minute
GOOGLE_PROMPT_TYPE=select_account consent       # Force consent screen

# Amazon SES Configuration
SES_ACCESS_KEY_ID=<SES_ACCESS_KEY_ID>
SES_SECRET_ACCESS_KEY=<SES_SECRET_ACCESS_KEY>
SES_REGION="eu-north-1"
